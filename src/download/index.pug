//- index.pug
extends ../_includes/layout.pug

block head
  title Your Icons are Ready

//- block meta pulls in here

block content

  //- this hides the nav and buy btn for this page
  style.
    .section-nav, .buy-btn {
      display: none;
    }

  section#icons.solid
    div.main-wrapper
      div.trigger
      div.section-split
        div.section-left
          div.section-text-wrapper
            h2 Your Icons are Ready
        div.section-right
          p Thanks so much for joining the Streamline list. Your free icon set is ready! <br/><br/>

          div.section-right-container
            a(href="http://streamlineicons.com/_assets/freebie/Streamline_Preview_Pack.zip").big-cta &darr; Download (12.6 MB)

          div.section-right-icon-wrapper#interface
            //- h3 User Interface
            //- h4 58 sets  <em>â€“</em>  954 icons
            div.section-right-container
              ul.icons--light.icons-showing
                li: div.svg-wrapper: svg: use(xlink:href="#icon-category-01-interface-light-alert-message-laptop")
                li: div.svg-wrapper: svg: use(xlink:href="#icon-category-01-interface-light-cog-double-3")
                li: div.svg-wrapper: svg: use(xlink:href="#icon-category-01-interface-light-face-id-3")
